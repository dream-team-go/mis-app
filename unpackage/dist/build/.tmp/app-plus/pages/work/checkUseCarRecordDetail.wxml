<view class="data-v-ccd6deb0"><cu-custom vue-id="1" bgColor="bg-linear-blue" isBack="{{true}}" class="data-v-ccd6deb0" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText" class="data-v-ccd6deb0">返回</view><view slot="content" class="data-v-ccd6deb0">车辆预约详情</view></cu-custom><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">预约进度</view></view><view class="bg-white padding data-v-ccd6deb0"><view class="cu-steps data-v-ccd6deb0"><block wx:for="{{steps}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item data-v-ccd6deb0',item.color]}}"><text class="cuIcon-roundcheckfill data-v-ccd6deb0"></text>{{''+item.name+''}}</view></block></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">用车事由：<text class="text-bold data-v-ccd6deb0">{{info.reason}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">用车人：<text class="text-bold data-v-ccd6deb0">{{info.car_user+"("+info.phone+")"}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">乘车人数：<text class="text-bold data-v-ccd6deb0">{{info.people_num}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">上车地点：<text class="text-bold data-v-ccd6deb0">{{info.start_place}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">用车区域：<text class="text-bold data-v-ccd6deb0">{{info.area}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">目的地：<text class="text-bold data-v-ccd6deb0">{{info.end_city+info.end_area+info.end_place}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">出发时间：<text class="text-bold data-v-ccd6deb0">{{info.predict_start_time}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">回程时间：<text class="text-bold data-v-ccd6deb0">{{info.predict_end_time}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">预约时间：<text class="text-bold data-v-ccd6deb0">{{info.apply_time}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">用车类型：<text class="text-bold data-v-ccd6deb0">{{info.type==1?"公务用车":"网约车"}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-ccd6deb0"><view class="action data-v-ccd6deb0">预约人：<text class="text-bold data-v-ccd6deb0">{{info.apply_user_name+"("+info.apply_user_phone+")"}}</text></view></view><view class="bottom-btns-seat data-v-ccd6deb0"></view><block wx:if="{{info.status==0}}"><view class="bottom-btns data-v-ccd6deb0"><view data-event-opts="{{[['tap',[['verifyFail',['$event']]]]]}}" class="cancel data-v-ccd6deb0" bindtap="__e">审批不通过</view><view data-event-opts="{{[['tap',[['verifySuccess',['$event']]]]]}}" class="pass data-v-ccd6deb0" bindtap="__e">审批通过</view></view></block></view>