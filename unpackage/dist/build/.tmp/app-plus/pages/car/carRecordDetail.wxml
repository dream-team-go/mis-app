<view class="data-v-34bad4e8"><cu-custom vue-id="1" bgColor="bg-linear-blue" isBack="{{true}}" class="data-v-34bad4e8" bind:__l="__l" vue-slots="{{['backText','content','right','right']}}"><view slot="backText" class="data-v-34bad4e8">返回</view><view slot="content" class="data-v-34bad4e8">车辆预约详情</view><view hidden="{{!(info.status==0)}}" slot="right" data-event-opts="{{[['tap',[['toEdit',['$event']]]]]}}" bindtap="__e" class="data-v-34bad4e8">修改</view><view hidden="{{!(info.status>1)}}" slot="right" data-event-opts="{{[['tap',[['toDispatch',['$event']]]]]}}" bindtap="__e" class="data-v-34bad4e8">派车单</view></cu-custom><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">预约进度</view></view><view class="bg-white padding data-v-34bad4e8"><view class="cu-steps data-v-34bad4e8"><block wx:for="{{steps}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item data-v-34bad4e8',item.color]}}"><text class="cuIcon-roundcheckfill data-v-34bad4e8"></text>{{''+item.name+''}}</view></block></view></view><block wx:if="{{info.status==-2}}"><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">取消订单原因：<text class="text-red data-v-34bad4e8">{{info.fail_reason}}</text></view></view></block><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">用车事由：<text class="text-bold data-v-34bad4e8">{{info.reason}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">用车人：<text class="text-bold data-v-34bad4e8">{{info.car_user+"("+info.phone+")"}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">乘车人数：<text class="text-bold data-v-34bad4e8">{{info.people_num}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">上车地点：<text class="text-bold data-v-34bad4e8">{{info.start_place}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">用车区域：<text class="text-bold data-v-34bad4e8">{{info.area}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">目的地：<text class="text-bold data-v-34bad4e8">{{info.end_city+info.end_area+info.end_place}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">出发时间：<text class="text-bold data-v-34bad4e8">{{info.predict_start_time}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">回程时间：<text class="text-bold data-v-34bad4e8">{{info.predict_end_time}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">预约时间：<text class="text-bold data-v-34bad4e8">{{info.apply_time}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">用车类型：<text class="text-bold data-v-34bad4e8">{{info.type==1?"公务用车":"网约车"}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-34bad4e8"><view class="action data-v-34bad4e8">预约人：<text class="text-bold data-v-34bad4e8">{{info.apply_user_name+"("+info.apply_user_phone+")"}}</text></view></view><block wx:if="{{info.status==0}}"><view class="bottom-btns-seat data-v-34bad4e8"></view></block><view data-event-opts="{{[['tap',[['cancleBook',['$event']]]]]}}" class="bottom-cancel-btn data-v-34bad4e8" bindtap="__e">取消预定</view></view>