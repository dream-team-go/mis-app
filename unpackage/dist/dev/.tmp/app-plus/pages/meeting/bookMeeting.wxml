<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">预定会议室</view></cu-custom><form hidden="{{!(isShowBottomModal==false)}}"><view class="cu-form-group"><view class="title">姓名</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','user_name','$event',[]],['para']]]]]}}" value="{{para.user_name}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','user_tel','$event',[]],['para']]]]]}}" value="{{para.user_tel}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">日期</view><picker mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+date+''}}</view></picker></view><view class="cu-form-group"><view class="title">时间</view><picker mode="time" value="{{time}}" start="startTime" data-event-opts="{{[['change',[['TimeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+time+''}}</view></picker></view><view class="cu-form-group"><view class="title">会议时长</view><picker value="{{hourIndex}}" range="{{hours}}" data-event-opts="{{[['change',[['ChangeHours',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(hourIndex>-1?hours[hourIndex]+"小时":"请选择")+''}}</view></picker></view><view class="cu-form-group"><view class="title">参会人数</view><picker value="{{peopleIndex}}" range="{{peoples}}" data-event-opts="{{[['change',[['ChangePeoples',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(peopleIndex>-1?peoples[peopleIndex]+"人":"请选择")+''}}</view></picker></view><view class="cu-form-group"><view class="title">会议室</view><view class="modal-group" data-target="Modal" data-event-opts="{{[['tap',[['showBottomModal',['$event']]]]]}}" bindtap="__e"><view class="picker">会议室</view></view></view><view class="cu-form-group"><view class="title">参会领导</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','attend_leader','$event',[]],['para']]]]]}}" value="{{para.attend_leader}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">备注</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','desc','$event',[]],['para']]]]]}}" value="{{para.desc}}" bindinput="__e"/></view></form><view class="{{['cu-modal bottom-modal',isShowBottomModal?'show':'']}}" id="meeting-list-modal"><view class="cu-dialog"><view class="cu-bar bg-gradual-blue" style="{{'padding-top:'+(StatusBar+'px')+';'+('height:'+(CustomBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['hideBottomModal',['$event']]]]]}}" class="action text-white" bindtap="__e">取消</view><view class="action text-white text-lg" style="text-align:center;margin-right:15px;">选择会议室</view><view class="action"></view></view><list class="cu-list menu text-left" style="{{'height:'+(ScreenHeight-CustomBar+'px')+';'}}" vue-id="2" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{100}}" wx:for-item="item" wx:for-index="index" wx:key="index"><cell class="cu-item arrow" vue-id="{{('3-'+index)+','+('2')}}" bind:__l="__l" vue-slots="{{['default']}}"><view>{{"Item "+(index+1)}}</view></cell></block></list></view></view></view>