<view><cu-custom vue-id="1" bgColor="bg-linear-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{isAdd?'预定用车':'编辑预定用车'}}</view></cu-custom><form><view class="cu-form-group"><view class="title">用车事由</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','reason','$event',[]],['para']]]]]}}" value="{{para.reason}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">用车人</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','car_user','$event',[]],['para']]]]]}}" value="{{para.car_user}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['para']]]]]}}" value="{{para.phone}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">乘车人数</view><picker value="{{peopleIndex}}" range="{{peoples}}" data-event-opts="{{[['change',[['ChangePeoples',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(peopleIndex>-1?peoples[peopleIndex]+"人":"请选择")+''}}</view></picker></view><view class="cu-form-group"><view class="title">上车地点</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','start_place','$event',[]],['para']]]]]}}" value="{{para.start_place}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">用车区域（市内/市外）</view><picker value="{{areaIndex}}" range="{{areas}}" data-event-opts="{{[['change',[['ChangeAreas',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+areas[areaIndex]+''}}</view></picker></view><view class="cu-form-group"><view class="title">目的地（市/县）</view><picker mode="multiSelector" value="{{multiIndex}}" range="{{multiArray}}" data-event-opts="{{[['change',[['MultiChange',['$event']]]],['columnchange',[['MultiColumnChange',['$event']]]]]}}" bindchange="__e" bindcolumnchange="__e"><view class="picker">{{''+multiArray[0][multiIndex[0]]+" "+multiArray[1][multiIndex[1]]+''}}</view></picker></view><view class="cu-form-group"><view class="title">目的地（详细地址）</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','end_place','$event',[]],['para']]]]]}}" value="{{para.end_place}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">出发日期</view><picker mode="date" value="{{date}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+date+''}}</view></picker></view><view class="cu-form-group"><view class="title">出发时间</view><picker mode="time" value="{{time}}" data-event-opts="{{[['change',[['TimeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+time+''}}</view></picker></view><view class="cu-form-group"><view class="title">回程日期</view><picker mode="date" value="{{backDate}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['BackDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+backDate+''}}</view></picker></view><view class="cu-form-group"><view class="title">回程时间</view><picker mode="time" value="{{backTime}}" data-event-opts="{{[['change',[['BackTimeChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+backTime+''}}</view></picker></view><view class="cu-form-group"><view class="title">用车类型（公务用车）</view><picker value="{{typeIndex}}" range="{{types}}" data-event-opts="{{[['change',[['ChangeTypes',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+types[typeIndex]+''}}</view></picker></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['Submit',['$event']]]]]}}" class="cu-btn bg-linear-blue margin-tb-sm lg" bindtap="__e">提交</button></view></form></view>