<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">用车评价</view></cu-custom><form><view class="cu-form-group feedback-title feedback-star-view"><text>评价分数</text><block wx:for="{{stars}}" wx:for-item="value" wx:for-index="key" wx:key="key"><text data-event-opts="{{[['tap',[['chooseStar',['$0'],[[['stars','',key]]]]]]]}}" class="{{[key<para.appraise_grade?'cuIcon-favorfill text-yellow':'cuIcon-favor text-gray']}}" bindtap="__e"></text></block></view><view class="cu-form-group"><view class="title">评价内容</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','zs_fee','$event',[]],['para']]]]]}}" value="{{para.zs_fee}}" bindinput="__e"/></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['Submit',['$event']]]]]}}" class="cu-btn bg-orange margin-tb-sm lg" bindtap="__e">提交</button></view></form></view>