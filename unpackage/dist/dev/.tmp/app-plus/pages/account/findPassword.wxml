<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">找回密码</view></cu-custom><form><view class="cu-form-group"><view class="title">手机号</view><input name="input" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view><view class="cu-form-group password-inut"><view class="title">新密码</view><input type="password" name="input" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="cu-form-group password-inut"><view class="title">重复密码</view><input type="password" name="input" data-event-opts="{{[['input',[['__set_model',['','rPassword','$event',[]]]]]]}}" value="{{rPassword}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">验证码</view><input name="input" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><button class="cu-btn bg-green shadow" disabled="{{disabled}}" data-event-opts="{{[['tap',[['SendCode',['$event']]]]]}}" bindtap="__e">{{"验证码"+authCode}}</button></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['Submit',['$event']]]]]}}" class="cu-btn bg-orange margin-tb-sm lg" bindtap="__e">提交</button></view></form></view>