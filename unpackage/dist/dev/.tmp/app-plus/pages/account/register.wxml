<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">注册</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title">机关事务局</view><picker value="{{sysIndex}}" range="{{sysOrganizations}}" range-key="org_name" data-event-opts="{{[['change',[['OrganizationChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(sysIndex>-1?sysOrganizations[sysIndex].org_name:'请选择')+''}}</view></picker></view><view class="cu-form-group"><view class="title">所属单位</view><picker value="{{oIndex}}" range="{{organizations}}" range-key="org_name" data-event-opts="{{[['change',[['OrganizationChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(oIndex>-1?organizations[oIndex].org_name:'请选择')+''}}</view></picker></view><view hidden="{{!(para.org_id.length>0)}}" class="cu-form-group"><view class="title">所属部门</view><picker value="{{dIndex}}" range="{{departments}}" range-key="name" data-event-opts="{{[['change',[['DepartmentChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(dIndex>-1?departments[dIndex].name:'请选择')+''}}</view></picker></view><view hidden="{{!(para.department_id>0)}}" class="cu-form-group"><view class="title">所属科室</view><picker value="{{sIndex}}" range="{{sections}}" range-key="name" data-event-opts="{{[['change',[['SectionChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(sIndex>-1?sections[sIndex].name:'请选择')+''}}</view></picker></view><view class="cu-form-group"><view class="title">是否司机</view><switch class="{{[isDriver?'checked':'']}}" checked="{{isDriver?true:false}}" data-event-opts="{{[['change',[['ChangeIsDriver',['$event']]]]]}}" bindchange="__e"></switch></view><view hidden="{{!(isDriver)}}" class="cu-form-group"><view class="title">驾龄</view><picker value="{{para.drive_age}}" range="{{driveAges}}" data-event-opts="{{[['change',[['ChangeDriveAge',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(para.drive_age>-1?driveAges[para.drive_age]+"年":"请选择")+''}}</view></picker></view><view hidden="{{!(isDriver)}}" class="cu-form-group"><view class="title">性别</view><switch class="{{['switch-sex',driverSex?'checked':'']}}" checked="{{driverSex?true:false}}" data-event-opts="{{[['change',[['ChangeDriverSex',['$event']]]]]}}" bindchange="__e"></switch></view><view hidden="{{!(isDriver)}}" class="cu-form-group"><view class="title">身份证号</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','id_card','$event',[]],['para']]]]]}}" value="{{para.id_card}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">手机号</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['para']]]]]}}" value="{{para.username}}" bindinput="__e"/></view><view class="cu-form-group password-inut"><view class="title">密码</view><input type="password" name="input" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['para']]]]]}}" value="{{para.password}}" bindinput="__e"/></view><view class="cu-form-group password-inut"><view class="title">重复密码</view><input type="password" name="input" data-event-opts="{{[['input',[['__set_model',['$0','rPassword','$event',[]],['para']]]]]}}" value="{{para.rPassword}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">验证码</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','code','$event',[]],['para']]]]]}}" value="{{para.code}}" bindinput="__e"/><button class="cu-btn bg-green shadow" disabled="{{disabled}}" data-event-opts="{{[['tap',[['SendCode',['$event']]]]]}}" bindtap="__e">{{"验证码"+authCode}}</button></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['Submit',['$event']]]]]}}" class="cu-btn bg-orange margin-tb-sm lg" bindtap="__e">提交</button></view></form></view>