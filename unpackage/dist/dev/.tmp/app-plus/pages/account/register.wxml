<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">注册</view></cu-custom><form><view class="cu-form-group margin-top"><view class="title">机关事务局</view><picker value="{{sysIndex}}" range="{{sysOrganizations}}" range-key="org_name" data-event-opts="{{[['change',[['OrganizationChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(sysIndex>-1?sysOrganizations[sysIndex].org_name:'请选择')+''}}</view></picker></view><view class="cu-form-group"><view class="title">所属单位</view><picker value="{{oIndex}}" range="{{organizations}}" range-key="org_name" data-event-opts="{{[['change',[['OrganizationChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(oIndex>-1?organizations[oIndex].org_name:'请选择')+''}}</view></picker></view><view hidden="{{!(para.org_id.length>0)}}" class="cu-form-group"><view class="title">所属部门</view><picker value="{{dIndex}}" range="{{departments}}" range-key="name" data-event-opts="{{[['change',[['DepartmentChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(dIndex>-1?departments[dIndex].name:'请选择')+''}}</view></picker></view><view hidden="{{!(para.department_id>0)}}" class="cu-form-group"><view class="title">所属科室</view><picker value="{{sIndex}}" range="{{sections}}" range-key="name" data-event-opts="{{[['change',[['SectionChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(sIndex>-1?sections[sIndex].name:'请选择')+''}}</view></picker></view></form></view>