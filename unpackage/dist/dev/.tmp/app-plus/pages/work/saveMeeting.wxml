e="{{'height:'+(ScreenHeight-CustomBar+'px')+';'}}" id="list-view"><view class="cu-list menu text-left"><block wx:for="{{$root.l0}}" wx:for-item="building" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['getBuilding',['$0'],[[['buildings','id',building.$orig.id]]]]]]]}}" class="cu-item arrow" style="padding-top:10rpx;padding-bottom:10rpx;" bindtap="__e"><view class="content"><view class="text-grey">{{building.$orig.name}}</view><view class="text-gray text-sm flex"><view class="text-cut">{{''+building.$orig.address+''}}</view></view></view><view class="action"><view class="text-grey text-xs">{{"建于"+building.$orig.build_time}}</view><view class="cu-tag round bg-orange sm">{{building.g0}}</view></view></view></block></view><uni-load-more vue-id="2" status="{{status}}" content-text="{{contentText}}" bind:__l="__l"></uni-load-more></view></view></view><view class="{{['building-list-modal cu-modal bottom-modal',isShowRoomModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-gradual-blue" style="{{'padding-top:'+(StatusBar+'px')+';'+('height:'+(CustomBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['hideRoomModal',['$event']]]]]}}" class="action text-white" bindtap="__e">取消</view><view class="action text-white text-lg" style="text-align:center;margin-right:15px;">选择办公房间</view><view class="action"></view></view><view style="{{'height:'+(ScreenHeight-CustomBar+'px')+';'}}" id="list-view"><view class="cu-list menu text-left"><block wx:for="{{$root.l1}}" wx:for-item="room" wx:for-index="__i1__" wx:key="id"><view data-event-opts="{{[['tap',[['getRoom',['$0'],[[['rooms','id',room.$orig.id]]]]]]]}}" class="cu-item arrow" style="padding-top:10rpx;padding-bottom:10rpx;" bindtap="__e"><view class="content"><view class="text-grey">{{room.$orig.number}}</view></view><view class="action"><view class="cu-tag round bg-orange sm">{{room.g1}}</view></view></view></block></view><uni-load-more vue-id="3" status="{{status}}" content-text="{{contentText}}