<view class="data-v-091070e2"><cu-custom vue-id="1" bgColor="bg-linear-blue" isBack="{{true}}" class="data-v-091070e2" bind:__l="__l" vue-slots="{{['backText','content','right']}}"><view slot="backText" class="data-v-091070e2">返回</view><view slot="content" class="data-v-091070e2">车辆维修申请详情</view><view hidden="{{!(info.status>=3||info.status<=-2)}}" slot="right" data-event-opts="{{[['tap',[['toRepairOrder',['$event']]]]]}}" bindtap="__e" class="data-v-091070e2">维修单</view></cu-custom><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action data-v-091070e2">维修申请进度</view></view><view class="bg-white padding data-v-091070e2"><view class="cu-steps data-v-091070e2"><block wx:for="{{steps}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item data-v-091070e2',item.color]}}"><text class="cuIcon-roundcheckfill data-v-091070e2"></text>{{''+item.name+''}}</view></block></view></view><block wx:if="{{info.status<0}}"><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action data-v-091070e2">不通过原因：<text class="text-red data-v-091070e2">{{info.fail_reason}}</text></view></view></block><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action data-v-091070e2">编号：<text class="text-bold data-v-091070e2">{{info.order_code}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action data-v-091070e2">车辆：<text class="text-bold data-v-091070e2">{{info.brand+" "+info.car_number}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action data-v-091070e2">申请人：<text class="text-bold data-v-091070e2">{{info.user_name+"("+info.user_phone+")"}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action data-v-091070e2">类型：<text class="text-bold data-v-091070e2">{{info.type==1?"维修":"保养"}}</text></view></view><view class="cu-bar bg-white solid-bottom data-v-091070e2"><view class="action dat