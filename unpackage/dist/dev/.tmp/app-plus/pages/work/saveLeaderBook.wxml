<view><cu-custom vue-id="1" bgColor="bg-linear-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">领导订餐</view></cu-custom><form><view class="cu-form-group"><view class="title">用餐领导</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','lead','$event',[]],['para']]]]]}}" value="{{para.lead}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">用餐人数</view><picker value="{{peopleIndex}}" range="{{peoples}}" data-event-opts="{{[['change',[['ChangePeoples',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(peopleIndex>-1?peoples[peopleIndex]+"人":"请选择")+''}}</view></picker></view><view class="cu-form-group"><view class="title">午餐/晚餐</view><picker value="{{typeIndex}}" range="{{types}}" data-event-opts="{{[['change',[['ChangeType',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+(typeIndex>-1?types[typeIndex]:"请选择")+''}}</view></picker></view><view class="cu-form-group"><view class="title">用餐日期</view><input name="input" disabled data-event-opts="{{[['input',[['__set_model',['$0','dining_date','$event',[]],['para']]]]]}}" value="{{para.dining_date}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">用餐要求</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','desc','$event',[]],['para']]]]]}}" value="{{para.desc}}" bindinput="__e"/></view><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['Submit',['$event']]]]]}}" class="cu-btn bg-linear-blue margin-tb-sm lg" bindtap="__e">提交</button></view></form></view>