<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">车辆调度</view></cu-custom><form><view class="cu-bar bg-white solid-bottom"><view class="action"><text class="cuIcon-title text-orange"></text>用车人数：<text class="text-bold">{{info.people_num}}</text></view></view><view class="cu-form-group"><view class="title">派车数量</view><picker value="{{numberIndex}}" range="{{numbers}}" data-event-opts="{{[['change',[['ChangeNumber',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+numbers[numberIndex]+'辆'}}</view></picker></view><block wx:for="{{para.list}}" wx:for-item="dispatchOrder" wx:for-index="index" wx:key="id"><view class="cu-item arrow margin-top"><view class="cu-bar bg-white solid-bottom"><view class="action"><text class="cuIcon-title text-orange"></text>{{'派车单'+(index+1)+''}}</view></view><view class="cu-form-group"><view class="title">乘车人数</view><picker id="{{index}}" value="{{dispatchOrder.people_num}}" range="{{peopleNums}}" data-event-opts="{{[['change',[['ChangePeopleNum',['$event']]]]]}}" bindchange="__e"><view class="picker">{{''+dispatchOrder.people_num+''}}</view></picker></view><view class="cu-form-group"><view class="title">车辆</view><view class="modal-group" data-target="Modal" data-event-opts="{{[['tap',[['showCarModal',[index]]]]]}}" bindtap="__e"><view class="picker">{{''+(dispatchOrder.car_id>0?dispatchOrder.brand+" "+dispatchOrder.color+" "+dispatchOrder.seat_num+"座"+" "+dispatchOrder.car_number:'请选择')+''}}</view></view></view><view class="cu-form-group"><view class="title">司机</view><view class="modal-group" data-target="Modal" data-event-opts="{{[['tap',[['showDriverModal',[index]]]]]}}" bindtap="__e"><view class="picker">{{''+(dispatchOrder.driver_id.length>0?dispatchOrder.driver_name+"("+dispatchOrder.driver_phone+")":'请选择')+''}}</view></view></view><view class="cu-form-group"><view class="title">备注</view><input name="input" data-event-opts="{{[['input',[['__set_model',['$0','bak','$event',[]],['para.list.'+index+'']]]]]}}" value="{{para.list[index].bak}}" bindinput="__e"/></view></view></block><view class="padding flex flex-direction"><button data-event-opts="{{[['tap',[['Submit',['$event']]]]]}}" class="cu-btn bg-orange margin-tb-sm lg" bindtap="__e">提交</button></view></form><view class="{{['list-modal cu-modal bottom-modal',isShowCarModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-gradual-blue" style="{{'padding-top:'+(StatusBar+'px')+';'+('height:'+(CustomBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['hideCarModal',['$event']]]]]}}" class="action text-white" bindtap="__e">取消</view><view class="action text-white text-lg" style="text-align:center;margin-right:15px;">选择车辆</view><view class="action"></view></view><view style="{{'height:'+(ScreenHeight-CustomBar+'px')+';'}}" id="list-view"><view class="cu-list menu text-left"><block wx:for="{{cars}}" wx:for-item="car" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['getCar',['$0'],[[['cars','id',car.id]]]]]]]}}" class="cu-item arrow" style="padding-top:10rpx;padding-bottom:10rpx;" bindtap="__e"><view class="content"><view class="text-grey">{{car.car_number}}</view><view class="text-gray text-sm flex"><view class="text-cut">{{''+car.brand+" "+car.color+''}}</view></view></view><view class="action"><view class="text-grey text-xs">{{car.seat_num+"座"}}</view><view class="cu-tag round bg-orange sm">{{car.is_clash>0?"已被调度":"可调度"}}</view></view></view></block></view><uni-load-more vue-id="2" status="{{status}}" content-text="{{contentText}}" bind:__l="__l"></uni-load-more></view></view></view><view class="{{['list-modal cu-modal bottom-modal',isShowDriverModal?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-gradual-blue" style="{{'padding-top:'+(StatusBar+'px')+';'+('height:'+(CustomBar+'px')+';')}}"><view data-event-opts="{{[['tap',[['hideDriverModal',['$event']]]]]}}" class="action text-white" bindtap="__e">取消</view><view class="action text-white text-lg" style="text-align:center;margin-right:15px;">选择司机</view><view class="action"></view></view><view style="{{'height:'+(ScreenHeight-CustomBar+'px')+';'}}" id="list-view"><view class="cu-list menu text-left"><block wx:for="{{drivers}}" wx:for-item="driver" wx:for-index="__i1__" wx:key="id"><view data-event-opts="{{[['tap',[['getDriver',['$0'],[[['drivers','id',driver.id]]]]]]]}}" class="cu-item arrow" style="padding-top:10rpx;padding-bottom:10rpx;" bindtap="__e"><view class="content"><view>{{driver.user_cn_name+"("+(driver.sex==1?'男':'女')+")"}}</view><view class="text-gray text-sm flex"><view class="text-cut">{{''+driver.tel_no+''}}</view></view></view><view class="action"><view class="text-grey text-xs">{{"驾龄"+driver.drive_age+"年"}}</view><view class="cu-tag round bg-orange sm">{{driver.is_clash>0?"已被调度":"可调度"}}</view></view></view></block></view><uni-load-more vue-id="3" status="{{status}}" content-text="{{contentText}}" bind:__l="__l"></uni-load-more></view></view></view></view>