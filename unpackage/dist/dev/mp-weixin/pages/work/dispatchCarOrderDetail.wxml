<view><cu-custom vue-id="30d661e8-1" bgColor="bg-linear-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content','right']}}"><view slot="backText">返回</view><view slot="content">派车单详情</view><view hidden="{{!(info.status==2||info.status==3)}}" slot="right" data-event-opts="{{[['tap',[['sureBack',['$event']]]]]}}" bindtap="__e">{{info.status==2?"确认归队":"修改费用"}}</view></cu-custom><view class="cu-bar bg-white solid-bottom"><view class="action">派车进度</view></view><view class="bg-white padding"><view class="cu-steps"><block wx:for="{{steps}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item',item.color]}}"><text class="cuIcon-roundcheckfill"></text>{{''+item.name+''}}</view></block></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">乘车人：<text class="text-bold">{{applyInfo.car_user+"（"+applyInfo.phone+"）"}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">乘车人数：<text class="text-bold">{{info.people_num}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">乘车地：<text class="text-bold">{{applyInfo.start_place}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">目的地：<text class="text-bold">{{applyInfo.end_place}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">乘车时间：<text class="text-bold">{{applyInfo.predict_start_time}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">返程时间：<text class="text-bold">{{applyInfo.predict_end_time}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">车辆：<text class="text-bold">{{info.brand+" "+info.color+" "+info.seat_num+"座"+" "+info.car_number}}</text></view></view><view class="cu-bar bg-white solid-bottom"><view class="action">司机：<text class="text-bold">{{info.driver_name+"("+info.driver_phone+")"}}</text></view></view><block wx:if="{{info.bak.length>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">备注：<text class="text-bold">{{info.bak}}</text></view></view></block><block wx:if="{{info.status>2}}"><uni-collapse vue-id="30d661e8-2" bind:__l="__l" vue-slots="{{['default']}}"><uni-collapse-item vue-id="{{('30d661e8-3')+','+('30d661e8-2')}}" title="{{$root.m0}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="cu-bar bg-white solid-bottom"><view class="action">里程费(元)：<text class="text-bold">{{info.stop_price+"/公里 x "+info.stop_num+"公里 = "+info.stop_price*info.stop_num}}</text></view></view><block wx:if="{{info.zs_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">住宿费(元)：<text class="text-bold">{{info.zs_fee}}</text></view></view></block><block wx:if="{{info.tc_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">停车费(元)：<text class="text-bold">{{info.tc_fee}}</text></view></view></block><block wx:if="{{info.hs_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">伙食费(元)：<text class="text-bold">{{info.hs_fee}}</text></view></view></block><block wx:if="{{info.jy_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">加油费(元)：<text class="text-bold">{{info.jy_fee}}</text></view></view></block><block wx:if="{{info.gl_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action"><text class="cuIcon-title text-orange"></text>过路费(元)：<text class="text-bold">{{info.gl_fee}}</text></view></view></block><block wx:if="{{info.xc_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">洗车费(元)：<text class="text-bold">{{info.xc_fee}}</text></view></view></block><block wx:if="{{info.clzy_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">车辆占用费(元)：<text class="text-bold">{{info.clzy_fee}}</text></view></view></block><block wx:if="{{info.other_fee>0}}"><view class="cu-bar bg-white solid-bottom"><view class="action">其它费用(元)：<text class="text-bold">{{info.other_fee}}</text></view></view></block></uni-collapse-item></uni-collapse></block></view>